<div class="col-lg-12 mx-lg-auto" style="margin-top: 10px;">
  
      <% @plans.each do |plan| %>
        <div class="card shadow">
          <div class="card-header"><h2 class="card-title text-center">Plans</h2></div>
          <div class="card body" style="padding: 10px;">
            <%= plan.name %>
            <%= plan.price %>
          </div>
        </div>
      <% end %>
</div>
